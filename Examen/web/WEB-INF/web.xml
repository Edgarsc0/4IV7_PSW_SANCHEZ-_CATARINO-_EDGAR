<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registrar</servlet-name>
        <servlet-class>Registrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidarLogin</servlet-name>
        <servlet-class>ValidarLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LevantarReporte_docente</servlet-name>
        <servlet-class>LevantarReporte_docente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubirReporte_docente</servlet-name>
        <servlet-class>SubirReporte_docente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>inicio_admin</servlet-name>
        <servlet-class>inicio_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DarDeAltaP</servlet-name>
        <servlet-class>DarDeAltaP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registrar</servlet-name>
        <url-pattern>/Registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidarLogin</servlet-name>
        <url-pattern>/ValidarLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LevantarReporte_docente</servlet-name>
        <url-pattern>/LevantarReporte_docente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubirReporte_docente</servlet-name>
        <url-pattern>/SubirReporte_docente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>inicio_admin</servlet-name>
        <url-pattern>/inicio_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DarDeAltaP</servlet-name>
        <url-pattern>/DarDeAltaP</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
